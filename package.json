{
  "name": "vscextension",
  "displayName": "vscExtension",
  "description": "vscExtension with react",
  "version": "0.0.1",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscExtension.showWebview"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscExtension.showWebview",
        "title": "Open vscExtension Panel"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscExtension",
          "title": "VSC Extension",
          "icon": "media/icon.png"
        }
      ]
    },
    "views": {
      "vscExtension": [
        {
          "type": "webview",
          "id": "vscExtension.quickAccess",
          "name": "Quick Access",
          "when": "true"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc && node esbuild.js",
    "watch": "tsc --watch",
    "pretest": "npm run compile",
    "test": "vscode-test",
    "build": "npm run compile && cd src/ui && npm run build && cd ../.."
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/node": "^22.13.1",
    "@types/vscode": "^1.96.0",
    "esbuild": "^0.24.2",
    "typescript": "^5.7.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "cheerio": "^1.1.2"
  }
}
